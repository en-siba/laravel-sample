{
"executionRoleArn": "arn:aws:iam::121452417296:role/ecsTaskExecutionRole",
"containerDefinitions": [
    {
    "name": "web",
    "image": "<WEB_IMAGE_NAME>",
    "portMappings": [
        {
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
        }
    ],
    "essential": true
    },
    {
        "name": "app",
        "image": "<APP_IMAGE_NAME>",
        "portMappings": [
            {
            "containerPort": 9000,
            "hostPort": 9000,
            "protocol": "tcp"
            }
        ],
        "essential": true
        }
],
"requiresCompatibilities": ["FARGATE"],
"networkMode": "awsvpc",
"cpu": "256",
"memory": "512",
"family": "testtask"
}
